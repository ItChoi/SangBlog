<mapper namespace="com.blog.sang.sequence.mapper.SequenceMapper">
	
	<select id="getCount" parameterType="String" resultType="Long">
		SELECT 
			COUNT
		FROM
			SEQUENCE_TABLE
		WHERE
			SEQUENCE_NAME = #{sequenceName}
	</select>
	
	<insert id="insert">
		INSERT INTO SEQUENCE_TABLE (SEQUENCE_ID, SEQUENCE_NAME, COUNT)
		VALUES (#{sequenceId}, #{sequenceName}, #{count})
	
		
	</insert>
	
	
	<update id="update">
		
	
	</update>
	
</mapper>